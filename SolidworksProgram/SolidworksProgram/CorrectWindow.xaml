<Window x:Class="SolidworksProgram.CorrectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SolidworksProgram"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="更正长度界面" Height="150" Width="300">
    <StackPanel>
        <GroupBox Header="变更数值填写框(请输入该机械臂正确的长度)">
            <TextBox x:Name="CorrectValueBox" FontSize="20" Height="40" Margin="0"/>
        </GroupBox>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button Grid.Column="0" Content="确定" Height="40" Margin="5" Click="ComfirmCorrectClick"/>
            <Button Grid.Column="1" Content="返回" Height="40" Margin="5" Click="ExitClick"/>
        </Grid>
    </StackPanel>
</Window>
